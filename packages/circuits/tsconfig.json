{
  "compilerOptions": {
    "target": "es2018",
    "module": "commonjs",
    "strict": true,
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "declaration": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "exclude": ["node_modules/**"],
  "include": ["./src", "./config", "./provers"]
}
